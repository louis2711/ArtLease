<div class="reviews mt-4">
  <h5 class="review-titles" ><strong>Add a review</strong></h5>
  <%= simple_form_for [artpiece, review] do |f| %>
    <%= f.input :rating, collection: 1..5, class: "review-titles"%>
    <%= f.input :content, class: "review-titles"%>
    <%= f.submit class: "btn"%>
  <% end %>
</div>
